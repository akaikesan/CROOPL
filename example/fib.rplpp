class Fib
    int[] xs
    
    method init()
        new int[2] xs
  
    method fib(int n)
        if n = 0 then
            xs[0] ^= 1
       	    xs[1] ^= 1
        else 
            print "hello"
            n -= 1
            call fib(n)
            xs[0] += xs[1]
            xs[0] <=> xs[1]
            print xs
        fi xs[0] = xs[1]

     method get (int out)
        out ^= xs[1]
  
class Program
